{
    "name": "%composername%",
    "type": "laravel-plugin",
    "authors": [
        {
            "name": "%name%",
            "email": "%email%"
        }
    ],
    "require": {
        "lassehaslev/laravel-package-router": "0.x"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.x",
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "laravel-modules": {
            "service-provider": "%namespace%\\Providers\\ServiceProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "%namespace%\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
